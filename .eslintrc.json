{
  "extends": ["plugin:react/recommended", "@slikts/prettier"],
  "rules": {
    "no-void": "off",
    "no-param-reassign": "off",
    "react/prop-types": "off",
    "no-return-assign": "off",
    "react/jsx-no-undef": "off",
    "import/prefer-default-export": "off"
  },
  "globals": {
    "globalThis": "writable"
  },
  "env": {
    "browser": true
  }
}
